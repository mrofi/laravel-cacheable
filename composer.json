{
    "name": "suitmedia/laravel-cacheable",
    "description": "Decorate your repositories and make them cacheable",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/suitmedia/laravel-cacheable",
    "keywords": [
        "laravel",
        "cache",
        "repository-pattern"
    ],
    "authors": [
        {
            "name": "Richan Fongdasen",
            "email": "richan.fongdasen@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/suitmedia/laravel-cacheable/issues",
        "source": "https://github.com/suitmedia/laravel-cacheable"
    },
    "require": {
        "php": "^7.2",
        "illuminate/cache": "5.8.*|^6.0|^7.0",
        "illuminate/database": "5.8.*|^6.0|^7.0",
        "illuminate/support": "5.8.*|^6.0|^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.8|~4.0|~5.0",
        "orchestra/database": "~3.8|~4.0|~5.0",
        "phpunit/phpunit": "^9.0 || ^8.0 || ~7.0 || ~6.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Suitmedia\\Cacheable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Suitmedia\\Cacheable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Suitmedia\\Cacheable\\ServiceProvider"
            ],
            "aliases": {
                "Cacheable": "Suitmedia\\Cacheable\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
